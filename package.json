{
  "name": "@powerfulyang/gltfjsx",
  "version": "6.2.12",
  "description": "GLTF to JSX converter",
  "scripts": {
    "build": "rollup -c",
    "test": "node src/test"
  },
  "type": "module",
  "keywords": [
    "gltf",
    "jsx",
    "react",
    "fiber",
    "three",
    "threejs",
    "webp"
  ],
  "author": "Paul Henschel",
  "maintainers": [
    "Max Rusan"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/gltfjsx.git"
  },
  "bugs": {
    "url": "https://github.com/pmndrs/gltfjsx/issues"
  },
  "homepage": "https://github.com/pmndrs/gltfjsx#readme",
  "bin": {
    "gltfjsx": "./cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@gltf-transform/core": "3.4.8",
    "@gltf-transform/extensions": "3.4.8",
    "@gltf-transform/functions": "3.4.8",
    "draco3dgltf": "1.5.6",
    "keyframe-resample": "0.0.15",
    "meow": "12.0.1",
    "meshoptimizer": "0.19.0",
    "prettier": "3.0.0",
    "read-pkg-up": "10.0.0",
    "three": "0.152.2",
    "three-stdlib": "2.22.4"
  },
  "optionalDependencies": {
    "jsdom": "22.0.0",
    "jsdom-global": "3.0.2",
    "sharp": "0.32.3"
  },
  "devDependencies": {
    "@types/node": "20.4.2",
    "lint-staged": "13.2.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-three-fiber"
  }
}
